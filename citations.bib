@article{Kanhabua:2017,
    Author = "Kanhabua, Nattiya and Kemkes, Philipp and Nejdl, Wolfgang and Nguyen, Tu Ngoc and Reis, Felipe and Tran, Nam Khanh",
    Abstract = "Significant parts of cultural heritage are produced on the web during the last decades. While easy accessibility to the current web is a good baseline, optimal access to the past web faces several challenges. This includes dealing with large-scale web archive collections and lacking of usage logs that contain implicit human feedback most relevant for today's web search. In this paper, we propose an entity-oriented search system to support retrieval and analytics on the Internet Archive. We use Bing to retrieve a ranked list of results from the current web. In addition, we link retrieved results to the WayBack Machine; thus allowing keyword search on the Internet Archive without processing and indexing its raw archived content. Our search system complements existing web archive search tools through a user-friendly interface, which comes close to the functionalities of modern web search engines (e.g., keyword search, query auto-completion and related query suggestion), and provides a great benefit of taking user feedback on the current web into account also for web archive search. Through extensive experiments, we conduct quantitative and qualitative analyses in order to provide insights that enable further research on and practical applications of web archives.",
    Date-Added = "2019-01-04 12:49:22 -0500",
    Date-Modified = "2019-01-04 13:23:35 -0500",
    Eprint = "1701.08256",
    Journal = "20th International Conference on Theory and Practice of Digital Libraries, TPDL 2016, Proceedings, pp 147-160",
    Keywords = "Web Archives ; Search ; Bing ; Wikipedia ; Internet Archive",
    Month = "01",
    Title = "How to Search the Internet Archive Without Indexing It",
    Url = "https://arxiv.org/abs/1701.08256",
    Year = "2017",
    File = ":Kanhabua/How to Search the Internet Archive Without Indexing0.pdf",
    Timestamp = "2019-01-04"
}

